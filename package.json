{
  "name": "fis3-react",
  "version": "1.0.0",
  "description": "使用百度fis3构建react应用",
  "scripts": {
    "dist": "npm run clean && fis3 release prod -d ./dist && npm run distClean",
    "start": "fis3 server start -p 3000 --type node && fis3 release -wL -c",
    "stop": "fis3 server stop",
    "lint": "eslint ./src",
    "clean": "rimraf dist/*",
    "distClean": "rimraf dist/node_modules dist/src dist/static"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fis-scaffold/default.git"
  },
  "keywords": [
    "fis",
    "react",
    "redux",
    "react-router"
  ],
  "author": "MRLYJ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fis-scaffold/default/issues"
  },
  "dependencies": {
    "process": "^0.11.9",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.12.2",
    "fis-parser-babel-6.x": "^6.20.0",
    "fis-parser-less": "^0.1.3",
    "fis-spriter-csssprites": "^0.3.11",
    "fis3-hook-commonjs": "^0.1.25",
    "fis3-hook-node_modules": "^2.2.8",
    "fis3-packager-deps-pack": "^0.1.2",
    "fis3-parser-typescript": "^1.1.0",
    "fis3-postpackager-loader": "^2.1.4",
    "fis3-preprocessor-autoprefixer": "^0.1.1",
    "fis3-preprocessor-js-require-css": "^0.1.1",
    "fis3-preprocessor-js-require-file": "^0.1.3",
    "rimraf": "^2.5.4"
  }
}
